
{
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "essential": true,
      "name": "frontend",
      "image": "wedding/frontend/react",
      "portMappings": [
        {
          "hostPort": 3000,
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "frontend",
          "awslogs-region": "eu-west-2",
          "awslogs-stream-prefix": "frontend"
        }
      }
    }
  ],
  "family": "WeddingSite_web",
  "memory": 512,
  "cpu": 256,
  "requiresCompatibilities": [
    "FARGATE"
  ]
}